.checkbox-image {
  display: none;
}
/*
- Style each label that is directly after the input
- position: relative; will ensure that any position: absolute children will position themselves in relation to it
*/
.checkbox-image + label {
  height: 174px;
  width: 175px;
  display: block;
  position: relative;
  transition: all 0.4s;
  border: solid 5px #FFF;
  box-shadow: 0 0 1px #FFF;
  /* Soften the jagged edge */
  cursor: pointer;
}
/* Provide a border when hovered and when the checkbox before it is checked */
.checkbox-image + label:hover,
.checkbox-image:checked + label {
  background-color: #ececec;
  -moz-box-shadow: 0 0 10px 5px #999;
  box-shadow: 0 0 10px 5px #999;
}
/*
- Create a pseudo element :after when checked and provide a tick
- Center the content
*/
.checkbox-image:checked + label:after {
  font-family: FontAwesome;
  content: "\f046";
  /*content is required, though it can be empty - content: '';*/
  display: block;
  height: 1.3em;
  width: 0.6em;
  position: absolute;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  font-size: 3em;
  margin: auto;
  color: #5CB85C;
  text-shadow: 1px 1px #333;
}
